{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@prince-assessment/components/accordion": [
        "libs/components/src/lib/accordion/index.ts"
      ],
      "@prince-assessment/components/menu-bar": [
        "libs/components/src/lib/menu-bar/index.ts"
      ],
      "@prince-assessment/components/header": [
        "libs/components/src/lib/header/index.ts"
      ],
      "@prince-assessment/components/recents": [
        "libs/components/src/lib/recents/index.ts"
      ],
      "@prince-assessment/components/tab": [
        "libs/components/src/lib/tab/index.ts"
      ],
      "@prince-assessment/components/money-in-out-tabs": [
        "libs/components/src/lib/money-in-out-tabs/index.ts"
      ],
      "@prince-assessment/components/table": [
        "libs/components/src/lib/table/index.ts"
      ],
      "@prince-assessment/styles": ["libs/styles/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
